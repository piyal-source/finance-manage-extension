{
    "manifest_version": 2,
    "name": "Finance manager",
    "description": "Manage your expenses with this extension",
    "version": "1.0.0",
    "icons": {
        "128": "./icon-128.png",
        "48": "./icon-48.png",
        "16": "./icon-16.png"
    },
    "browser_action": {
        "default_icon": "./icon-16.png",
        "default_popup": "./popup.html",
        "default_title": "Finance manager"
    },
    "options_page": "./options.html",
    "permissions": [
        "storage"
    ]
}